
<div id="invoice-report-chart"></div>

<script type="text/javascript">
	new Morris.Line({
	  // ID of the element in which to draw the chart.
	  element: 'invoice-report-chart',
	  // Chart data records -- each entry in this array corresponds to a point on
	  // the chart.
	  data : [
	  <% for date in report.keys %>
	  {
      date: '<%= date %>', cost: <%= report[date]['cost'] %>,
      netto: '<%= report[date]['netto'] %>',
      profit: '<%= report[date]['profit'] %>',
      house: '<%= report[date]['house_commission'] %>',
      affiliate: '<%= report[date]['affiliate_commission'] %>'
      },
	  <% end %>
	  ],
	 
	  // The name of the data record attribute that contains x-values.
	  xkey: 'date',
	  // A list of names of data record attributes that contain y-values.
	  ykeys: ['netto', 'cost', 'profit', 'house', 'affiliate'],
	  // Labels for the ykeys -- will be displayed when you hover over the
	  // chart.
	  labels: ['Netto', 'Cost', 'Profit', 'House Commission', 'Affiliate Commission']
	});
</script>
