<%= form_for @format do |f| %>


<p>Return<br/>
  <%= f.number_field :return %>
</p>

<p>Product code<br/>
  <%= f.number_field :product_code %>
</p>

<p>Package height<br/>
  <%= f.number_field :package_height %>
</p>

<p>Package length<br/>
  <%= f.number_field :package_length %>
</p>

<p>Package width<br/>
  <%= f.number_field :package_width %>
</p>

<p>Package weight<br/>
  <%= f.number_field :package_weight%>
</p>

<h3>Sender address</h3>
<p>Company name<br/>
  <%= f.number_field :sender_company_name %>
</p>
<p>Attention<br/>
  <%= f.number_field :sender_attention %>
</p>
<p>Street address line 1<br/>
  <%= f.number_field :sender_address_line1 %>
</p>
