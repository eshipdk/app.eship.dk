  <%= f.number_field :recipient_phone_number %>
</p>
<p>Email<br/>
  <%= f.number_field :recipient_email %>
</p>

<p>Description<br/>
	<%= f.number_field :description %>
</p>

<p>Amount<br/>
	<%= f.number_field :amount %>
</p>

<p>Reference<br/>
	<%= f.number_field :reference %>
</p>

<p>
  <%= f.submit %>
</p>

<% end %>
